server
    listen 127.0.0.1:9099
    upload_file_to ./root/put_test/
    location /
        root ./root/StartPage/
        index index.html
        allow_methods GET
        auto_index off
    location /directory/
        root ./root/YoupiBanane/
        auto_index off
        index youpi.bad_extension
        allow_methods GET POST PUT
    location /put_test/
        root ./root/
        allow_methods GET PUT
        auto_index off
    location /post_body/
        root ./root/
        allow_methods GET POST
        max_body_size 100 B
        auto_index off